	{{#if page.attributes.robots}}
	<meta name="robots" content="{{page.attributes.robots}}">
	{{/if}}

	<meta property="og:site_name" content="Cloudflow" />
	<meta name="author" content="Cloudflow">
	<meta itemprop="image" content="{{or page.attributes.social-share-image "https://cloudflow.io/_/img/cloudflow-logo-social.jpg"}}">
	<meta name="twitter:image:src" content="{{or page.attributes.social-share-image "https://cloudflow.io/_/img/cloudflow-logo-social.jpg"}}">
	<meta property="og:image" content="{{or page.attributes.social-share-image "https://cloudflow.io/_/img/cloudflow-logo-social.jpg"}}" />
	<meta property="og:image:secure_url" content="{{or page.attributes.social-share-image "https://cloudflow.io/_/img/cloudflow-logo-social.jpg"}}" />